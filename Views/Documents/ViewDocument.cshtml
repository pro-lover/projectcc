@model CodeTheCloud.ViewModels.ViewDocumentViewModel

@{
    Layout = null;
}

@if (Model.Extension.Equals(".pdf"))
{
    <embed id ="pdfdiv" src=@Model.PathToFile type="application/pdf" width ="100%" height="600px"/> 
}
else if (Model.Extension.Equals(".png") || Model.Extension.Equals(".jpeg") || Model.Extension.Equals(".jpg"))
{
    <img src=@Model.PathToFile class="img-responsive" style="width: 100% ; height: auto"/>
}